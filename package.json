{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:backend dev:frontend",
    "lint": "eslint . && tsc",
    "build": "tsc && next build",
    "dev:backend": "convex dev",
    "dev:frontend": "next dev",
    "init": "convex import ./fullstack/initialData/snapshot-fullstack-convex.zip",
    "init:prod": "convex import --prod ./fullstack/initialData/snapshot-fullstack-convex.zip",
    "setup": "convex dev --until-success && npx @convex-dev/auth",
    "start": "next start"
  },
  "dependencies": {
    "@auth/core": "^0.34.2",
    "@convex-dev/auth": "^0.0.71",
    "convex": "^1.16.0",
    "next": "latest",
    "node-fetch": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-modal": "^3.16.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.3.0",
    "@types/node": "^18.11.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-modal": "^3.13.1",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.7.3"
  }
}
